{
  "name": "smine",
  "version": "1.2.6",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=dev nodemon ./bin/smine.js",
    "test": "NODE_ENV=test NODE_PATH=lib mocha --exit"
  },
  "preferGlobal": true,
  "bin": {
    "smine": "./bin/smine.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/safari12/smine.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/safari12/smine/issues"
  },
  "homepage": "https://github.com/safari12/smine#readme",
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "test/**/*.spec.js",
      "lib/logger.js",
      "lib/mailer.js",
      "lib/smine.js"
    ],
    "ignore-class-method": "methodToIgnore",
    "reporter": [
      "html",
      "text"
    ],
    "extension": [
      ".jsx"
    ],
    "cache": true,
    "all": true
  },
  "dependencies": {
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "got": "^9.6.0",
    "jsonschema": "^1.2.4",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "mongoose": "^5.4.12",
    "morgan": "^1.9.1",
    "node-schedule": "^1.3.2",
    "nodemailer": "^4.7.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "winston": "^3.2.1",
    "yargs": "^11.1.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-exclude": "^1.0.12",
    "chai-http": "^4.2.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^4.0.0",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0",
    "sinon": "^6.3.5",
    "sinon-chai": "^3.3.0"
  }
}
